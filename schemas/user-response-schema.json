{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "User Response Schema",
  "description": "Schema de resposta para dados de usu√°rio",
  "properties": {
    "id": {
      "type": "integer",
      "minimum": 1
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "name": {
      "type": "string",
      "minLength": 1
    },
    "createdAt": {
      "type": "string",
      "format": "date-time"
    },
    "updatedAt": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": ["id", "email", "name"],
  "additionalProperties": true
}
